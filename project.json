{
  "name": "@nihil_backend/post",
  "root": "post",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "post"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "post"
      }
    },
    "test:ci": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:ci",
        "cwd": "post"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run clean",
        "cwd": "post"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run lint", "cwd": "post" }
    }
  }
}
