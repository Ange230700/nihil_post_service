{
  "name": "@nihil_backend/post",
  "root": "post",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b",
        "cwd": "post"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
